{
    "manifestVersion": 1,
    "id": "vsts-work-sync",
    "publisher": "ZouJian",
    "version": "1.0.0.22",
    "name": "VSTS Work Sync",
    "description": "A sample Visual Studio Services extension",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "my-work-sync-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Work同步",
                "uri": "my-hub.html",
                "order":99
            }
        }
    ],
    "scopes": [
        "vso.work",
        "vso.project"
    ],
    "files": [
        {
            "path": "css",
            "addressable": true
        },
          {
            "path": "my-hub.html",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "dist",
            "addressable": true,
            "packagePath": "scripts"
        }
    ]
}